{
  "name": "react-native-saas-boilerplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "expo prebuild --clean",
    "build:android:debug": "expo run:android --variant debug",
    "build:android:release": "expo run:android --variant release",
    "build:ios:debug": "expo run:ios --configuration Debug",
    "build:ios:release": "expo run:ios --configuration Release",
    "build:web": "expo export --platform web",
    "build:web:debug": "expo export --platform web --no-minify",
    "preview:web": "npx serve dist",
    "install:android:debug": "adb -d install android/app/build/outputs/apk/debug/app-debug.apk",
    "install:android:release": "adb -d install android/app/build/outputs/apk/release/app-release.apk",
    "kill": "lsof -ti:8081 | xargs kill -9 || pkill -f 'expo start' || pkill -f 'metro'",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@prisma/client": "^6.19.2",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.91.0",
    "expo": "~54.0.31",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^6.2.0",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
